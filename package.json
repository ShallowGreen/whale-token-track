{
  "name": "whale-token-track",
  "version": "1.0.0",
  "description": "巨鲸钱包监控通知",
  "author": "ShallowGreen",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:release": "cross-env NODE_ENV=development pm2 start ./dist/main.js --name 'token-track'",
    "start:prod": "cross-env NODE_ENV=production pm2 start ./dist/main.js --name 'token-track'",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "delete": "pm2 delete token-track",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/bluebird": "^3.5.32",
    "@types/validator": "^13.1.0",
    "argon2": "^0.27.2",
    "aws-sdk": "^2.884.0",
    "bn.js": "^5.2.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "d": "^1.0.1",
    "ethereum-input-data-decoder": "^0.3.5",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mysql2": "^2.2.5",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^14.14.0",
    "@types/supertest": "^2.0.8",
    "eslint": "7.7.0",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
